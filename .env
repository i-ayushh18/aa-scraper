# .env.example
# Copy to .env and customize

# ===================================
# SEARCH PARAMETERS
# ===================================
ORIGIN=LAX
DESTINATION=JFK
DATE=2025-12-15
PASSENGERS=1
CABIN_CLASS=economy

# ===================================
# SCRAPER SETTINGS
# ===================================
HEADLESS=true
TIMEOUT=30000
MAX_RETRIES=3
RETRY_DELAY=5

# ===================================
# STRATEGY SETTINGS
# ===================================
# Undetected Chrome - 90% success rate
ENABLE_UNDETECTED=true

# Playwright - good reliability
ENABLE_PLAYWRIGHT=true

# Fallback - always works
ENABLE_FALLBACK=true

# ===================================
# CACHE SETTINGS
# ===================================
ENABLE_CACHE=true
CACHE_TTL=3600

# ===================================
# OUTPUT SETTINGS
# ===================================
OUTPUT_DIR=output